
{
    "exp_name": "BaseSDNE",
    "agent": "SDNEAgent",
    "mode": "train",
  
    "cuda": true,
    "seed": 1337,
  
    "nr_epochs" : 1000,
    "schedule_step_size" : 50,
    "schedule_gamma" : 0.99,
    "amsgrad" : false,
    "shuffle" : true,
    "batch_size" : 10,
    "learning_rate": 0.001,
    "weight_decay": 0.0001,
    "beta1": 2,
    "beta2": 0.999,

    "dim_intermediate" : 256,
    "dim_embedding" : 2,
    "activation" : "Tanh",
    "nr_encoders" : 3,
    "nr_decoders" : 3,


  
    "validate_every": 2,
    "log_interval": 10000,
  
    "checkpoint_file": "checkpoint.pth.tar",
  
    "data_loader_workers": 0,
    "pin_memory": true,
    "async_loading": true
  
  }